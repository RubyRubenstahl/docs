{
  "styles": {
    "website": "styles/website.css"
  },
  "plugins": [
    "feathers-collapsible-menu",
    "heading-anchors",
    "include-codeblock",
    "edit-link",
    "ga",
    "feathers-versions",
    "advanced-emoji",
    "codetabs"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/feathersjs/docs/edit/buzzard",
      "label": "Edit This Page",
      "placement": "left"
    },
    "ga": {
      "token": "UA-64904930-1"
    },
    "lunr": {
      "maxIndexSize": 1000000000
    },
    "menu": {
      "deep": true
    },
    "versions": {
      "gitbookConfigURL": "https://raw.githubusercontent.com/feathersjs/docs/master/book.json",
      "options": [
        {
          "value": "https://docs.feathersjs.com",
          "text": "Latest Stable - Auk (v2)"
        },
        {
          "value": "https://buzzard.docs.feathersjs.com",
          "text": "Next Release - Buzzard (v3)"
        },
        {
          "value": "https://legacy.docs.feathersjs.com",
          "text": "Legacy (v1)"
        }
      ]
    }
  }
}
